<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smartcollege">

    <application
        android:allowBackup="true"
        android:icon="@drawable/tec_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Menu_Croutine" />
        <activity android:name=".Menu_Gnotice" />
        <activity android:name=".Menu_EduFile" />
        <activity android:name=".FlashScreen"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".Navigation_Drawer"
            android:label="@string/title_activity_navigation__drawer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".HomePage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>